[access]
	inheritFrom = All-Projects
[access "refs/heads/*"]
	forgeAuthor = group ldap/opnfv-gerrit-fuel-contributors
        label-Code-Review = -2..+2 group ldap/opnfv-gerrit-fuel-submitters
        label-Code-Review = -1..+1 group Registered Users
        label-Workflow = -1..+1 group ldap/opnfv-gerrit-fuel-submitters
	label-Workflow = -1..+0 group Change Owner
        label-Verified = -1..+1 group ldap/opnfv-gerrit-fuel-submitters
        submit = group ldap/opnfv-gerrit-fuel-submitters
	forgeAuthor = group ldap/opnfv-gerrit-fuel-submitters
	pushMerge = group ldap/opnfv-gerrit-fuel-submitters
[access "refs/tags/*"]
        create = group ldap/opnfv-gerrit-fuel-submitters
        push = group ldap/opnfv-gerrit-fuel-submitters
        pushTag = group ldap/opnfv-gerrit-fuel-submitters
        pushSignedTag = group ldap/opnfv-gerrit-fuel-submitters
[label "Workflow"]
   function = MaxWithBlock
   value = -1 Work in Progress
   value =  0 Ready For Review
   value = +1 Approved
   defaultValue = 0
